\name{calc_ac_lifespan_convert}
\alias{calc_ac_lifespan_convert}
\title{Convert between age at other-cause death and time to other-cause death}
\description{Based on age at start of study, convert
between the age time scale and study time scale}
\usage{calc_ac_lifespan_convert(ocmat, age, from)}
\arguments{
  \item{ocmat}{Matrix of times. Rows are individuals}
  \item{age}{Vector or matrix of ages at study start}
  \item{from}{Either "age" or "time"}
}

\value{Matrix of same dimensions as ocmat}

\author{Jeanette Birnbaum & Leslie Mallinger}





