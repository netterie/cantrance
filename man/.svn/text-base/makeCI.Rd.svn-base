\name{makeCI}
\alias{makeCI}
\title{Compile mean estimate, lower, and upper confidence bounds into a single vector with appropriate formatting}

\usage{makeCI(estimate, lower, upper, fmat)}
\arguments{
  \item{estimate}{Numeric vector with mean estimate}
  \item{lower}{Numeric vector for lower bound}
  \item{upper}{Numeric vector for upper bound}
  \item{fmat}{Format for numbers in output, in the form accepted
by sprintf()}
}

\value{A vector}

\author{Jeanette Birnbaum & Leslie Mallinger}





