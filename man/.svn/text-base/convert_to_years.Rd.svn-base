\name{convert_to_years}
\alias{convert_to_years}
\title{Convert to years from other units}
\description{Converts from years, months, days, or hours}
\usage{convert_to_years(x, from = "days")}
\arguments{
  \item{x}{A numeric vector}
  \item{from}{One of "years", "months", "days", or "hours"}
}

\value{Numeric vector}

\author{Jeanette Birnbaum & Leslie Mallinger}





